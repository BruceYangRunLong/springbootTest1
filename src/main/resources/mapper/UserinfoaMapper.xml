<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboottest1.mapper.UserinfoaMapper">

    <select id="verifyUserinfoa" parameterType="com.example.springboottest1.entity.Userinfoa" resultType="com.example.springboottest1.entity.Userinfoa">
        select *
        from userinfoa
        where username =#{username} and password= #{password}
    </select>

</mapper>

